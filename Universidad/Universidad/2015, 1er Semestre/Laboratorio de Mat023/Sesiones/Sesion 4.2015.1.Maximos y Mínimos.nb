 Cell[BoxData[
  FormBox[
   SubscriptBox["x", "0"], TraditionalForm]]],
 ")= ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[Lambda]", "1"], TraditionalForm]]],
 " \[Del]",
 Cell[BoxData[
  FormBox[
   SubscriptBox["g", "1"], TraditionalForm]]],
 " (",
 Cell[BoxData[
  FormBox[
   SubscriptBox["x", "0"], TraditionalForm]]],
