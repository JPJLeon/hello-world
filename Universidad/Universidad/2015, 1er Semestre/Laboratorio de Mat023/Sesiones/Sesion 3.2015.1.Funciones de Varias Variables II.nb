 Cell[BoxData[
  FormBox[
   SubscriptBox["J", 
    SuperscriptBox["f", 
     RowBox[{"-", "1"}]]], TraditionalForm]]],
 "(",
 Cell[BoxData[
  FormBox[
   SubscriptBox["Q", "0"], TraditionalForm]]],
 ") =  ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox[
    RowBox[{"[", 
     RowBox[{
      SubscriptBox["J", "f"], "(", 
      SubscriptBox["P", "0"], ")"}], "]"}], 
    RowBox[{"-", "1"}]], TraditionalForm]]],
 "\n                       "
}], "Teorema",
 CellChangeTimes->{{3.6385306795034795`*^9, 3.6385306841598253`*^9}}],

