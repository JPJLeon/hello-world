Cell["Sistema de coordenadas.", "Subseccion"],

Cell["\<\
Ejercicio.
               SISTEMA DE COORDENADAS PARABOLICO
              Probar que la funci\[OAcute]n  ( u,v, \[Phi])  -> (x,y,z)  \
definida:
                             x = u v Cos[\[Phi]] ;  y  = u v Sin[\[Phi]]  ;  \
z = (v^2 - u^2)/2
             es localmente invertible.
             \
\>", "Ejercicio"],

Cell["Primero se introducen las funciones componentes:", "Text"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"x", "=", 
   RowBox[{"u", " ", "v", " ", 
    RowBox[{"Cos", "[", "\[Phi]", "]"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"y", "=", 
   RowBox[{"u", " ", "v", " ", 
    RowBox[{"Sin", "[", "\[Phi]", "]"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"z", "=", 
   RowBox[{
    RowBox[{"(", 
     RowBox[{
      SuperscriptBox["v", "2"], "-", 
      SuperscriptBox["u", "2"]}], ")"}], "/", "2"}]}], ";"}]}], "Input"],

Cell["Se calcula la matriz jacobiana J de la funcion", "Text"],

Cell[BoxData[
 RowBox[{"Jac", "=", 
  RowBox[{"(", GridBox[{
     {
      RowBox[{"D", "[", 
       RowBox[{"x", ",", "u"}], "]"}], 
      RowBox[{"D", "[", 
       RowBox[{"x", ",", "v"}], "]"}], 
      RowBox[{"D", "[", 
       RowBox[{"x", ",", "\[Phi]"}], "]"}]},
     {
      RowBox[{"D", "[", 
       RowBox[{"y", ",", "u"}], "]"}], 
      RowBox[{"D", "[", 
       RowBox[{"y", ",", "v"}], "]"}], 
      RowBox[{"D", "[", 
       RowBox[{"y", ",", "\[Phi]"}], "]"}]},
     {
      RowBox[{"D", "[", 
       RowBox[{"z", ",", "u"}], "]"}], 
      RowBox[{"D", "[", 
       RowBox[{"z", ",", "v"}], "]"}], 
      RowBox[{"D", "[", 
       RowBox[{"z", ",", "\[Phi]"}], "]"}]}
    }], ")"}]}]], "Input"],

Cell["Calculemos su determinante y simplifiquemos la expresi\[OAcute]n", \
"Text"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"J", "=", 
   RowBox[{"Det", "[", "Jac", "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"%", "//", "FullSimplify"}]}], "Input"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"Det", "[", 
    RowBox[{"Inverse", "[", "Jac", "]"}], "]"}], "//", "FullSimplify"}], "//",
   "MatrixForm"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"1", "/", "J"}], "//", "FullSimplify"}]}], "Input"],

