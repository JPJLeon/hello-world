               RowBox[{
                RowBox[{
                 RowBox[{"(", 
                  RowBox[{"1", "+", "#2"}], ")"}], "*", "dv"}], "+", "v1"}]}],
               "}"}], ",", 
             RowBox[{"{", 
              RowBox[{
               RowBox[{
                RowBox[{"#3", "*", "dw"}], "+", "w1"}], ",", 
               RowBox[{
                RowBox[{
                 RowBox[{"(", 
                  RowBox[{"1", "+", "#3"}], ")"}], "*", "dw"}], "+", "w1"}]}],
               "}"}]}], "}"}], "&"}], ",", 
          RowBox[{"Select", "[", 
           RowBox[{"cubes", ",", 
            RowBox[{
             RowBox[{"prepassCubeQ", "[", 
              RowBox[{"f", ",", "#"}], "]"}], "&"}]}], "]"}], ",", 
          RowBox[{"{", "1", "}"}]}], "]"}], "]"}]}]}], "]"}]}], 
   "\[IndentingNewLine]", "\n", 
   RowBox[{
    RowBox[{"finalBoxes", "[", 
     RowBox[{"func_", ",", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"u1_", ",", "u2_"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"v1_", ",", "v2_"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"w1_", ",", "w2_"}], "}"}]}], "}"}], ",", 
      RowBox[{"{", 
       RowBox[{"nu_", ",", "nv_", ",", "nw_"}], "}"}], ",", "cb_"}], "]"}], ":=", 
    RowBox[{"Module", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
       "f", ",", "du", ",", "dv", ",", "dw", ",", "coord", ",", "plate"}], 
       "}"}], ",", 
      RowBox[{
       RowBox[{"du", "=", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"u2", "-", "u1"}], ")"}], "/", "nu"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"dv", "=", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"v2", "-", "v1"}], ")"}], "/", "nv"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"dw", "=", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"w2", "-", "w1"}], ")"}], "/", "nw"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"f", "[", 
         RowBox[{"i_", ",", "j_", ",", "k_"}], "]"}], ":=", 
        RowBox[{
         RowBox[{"f", "[", 
          RowBox[{"i", ",", "j", ",", "k"}], "]"}], "=", 
         RowBox[{"func", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"i", "*", "du"}], "+", "u1"}], ",", 
           RowBox[{
            RowBox[{"j", "*", "dv"}], "+", "v1"}], ",", 
           RowBox[{
            RowBox[{"k", "*", "dw"}], "+", "w1"}]}], "]"}]}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"coord", "[", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{"i1_", ",", "j1_", ",", "k1_"}], "}"}], ",", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{"{", 
             RowBox[{"i2_", ",", "j2_", ",", "k2_"}], "}"}], ",", 
            RowBox[{"{", 
             RowBox[{"0", ",", "0", ",", "0"}], "}"}]}], "}"}]}], "]"}], ":=", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"u1", "+", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{"i1", "+", "i2"}], ")"}], "*", "du"}]}], ",", 
          RowBox[{"v1", "+", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{"j1", "+", "j2"}], ")"}], "*", "dv"}]}], ",", 
          RowBox[{"w1", "+", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{"k1", "+", "k2"}], ")"}], "*", "dw"}]}]}], "}"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"coord", "[", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{"i1_", ",", "j1_", ",", "k1_"}], "}"}], ",", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{"{", 
             RowBox[{"i2_", ",", "j2_", ",", "k2_"}], "}"}], ",", 
            RowBox[{"{", 
             RowBox[{"i3_", ",", "j3_", ",", "k3_"}], "}"}]}], "}"}]}], "]"}],
         ":=", 
        RowBox[{"Module", "[", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{
            RowBox[{"f1", "=", 
             RowBox[{"f", "[", 
              RowBox[{
               RowBox[{"i1", "+", "i2"}], ",", 
               RowBox[{"j1", "+", "j2"}], ",", 
               RowBox[{"k1", "+", "k2"}]}], "]"}]}], ",", "f2"}], "}"}], ",", 
          RowBox[{
           RowBox[{"f2", "=", 
            RowBox[{"f1", "/", 
             RowBox[{"(", 
              RowBox[{
               RowBox[{"f", "[", 
                RowBox[{
                 RowBox[{"i1", "+", "i2", "+", "i3"}], ",", 
                 RowBox[{"j1", "+", "j2", "+", "j3"}], ",", 
                 RowBox[{"k1", "+", "k2", "+", "k3"}]}], "]"}], "-", "f1"}], 
              ")"}]}]}], ";", "\[IndentingNewLine]", 
           RowBox[{"{", 
            RowBox[{
             RowBox[{"u1", "+", 
              RowBox[{"du", 
               RowBox[{"(", 
                RowBox[{"i1", "+", "i2", "-", 
                 RowBox[{"i3", "*", "f2"}]}], ")"}]}]}], ",", 
             RowBox[{"v1", "+", 
              RowBox[{"dv", 
               RowBox[{"(", 
                RowBox[{"j1", "+", "j2", "-", 
                 RowBox[{"j3", "*", "f2"}]}], ")"}]}]}], ",", 
             RowBox[{"w1", "+", 
              RowBox[{"dw", 
               RowBox[{"(", 
                RowBox[{"k1", "+", "k2", "-", 
                 RowBox[{"k3", "*", "f2"}]}], ")"}]}]}]}], "}"}]}]}], "]"}]}],
        ";", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"plate", "[", 
         RowBox[{"i_", ",", "j_", ",", "k_"}], "]"}], ":=", 
